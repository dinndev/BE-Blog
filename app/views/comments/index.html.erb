<h1>Comments at <%= @article.title %></h1>

<%= @comments.each do |comment| %>

<ul>
  <li>
    <p><%= comment.id %></p>
    <p><%= comment.body %></p>
    <%= link_to "Edit", edit_article_comment_path(@article) %>
  </li>
</ul>
<% end %> <%= link_to "Create comment", new_article_comment_path(@article) %>
